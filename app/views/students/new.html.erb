<ol><%= form_for @student do |f| %> 
    <ol><%= f.label :first_name %></ol>  
    <ol><%= f.text_field :first_name, value: @student.first_name%></ol>  
    <ol><%= f.label :last_name %></ol>  
    <ol><%= f.text_field :last_name, value: @student.last_name%></ol>  
    <ol><%= f.label :age %></ol>  
    <ol><%= f.number_field :age, value: @student.age%></ol>
    <ol><%= f.label :education %></ol>  
    <ol><%= f.text_field :education, value: @student.education%></ol>  
    <ol><%= f.label :cohort_id %></ol>  
    <ol><%= f.text_field :cohort_id, value: @student.cohort_id%></ol> 
    <p>
    <ol><%= f.submit %></ol>
<% end %>
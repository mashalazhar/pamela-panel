<div class="container">
<h2>Students</h2>          
<table class="table table-striped">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Age</th>
      <th>Education</th>
      <th>Cohort ID</th>
    </tr>
  </thead>
  <tbody>
  
    <tr>
    <% @students.each do |x| %>
      <td><%= x.first_name %></td>
      <td><%= x.last_name %></td>
      <td><%= x.age %></td>
      <td><%= x.education %></td>
      <td><%= x.cohort_id %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>
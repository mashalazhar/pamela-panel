    <div class="container">
    <h2>Courses</h2>          
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Course</th>
          <th>Name</th>
          <th>Total Hours</th>
          <th>Edit</th>
          <th>Delete</th>
        </tr>
      </thead>
      <tbody>
      
        <tr>
        <% @courses.each do |x| %>
          <td><%= x.id %></td>
          <td><%= x.name %></td>
          <td><%= x.total_hours %></td>
          <td><%= link_to 'Edit', edit_course_path(x), remote:true %></td>
          <td><%= link_to 'Delete', course_path(x), method: :delete, remote:true, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <div id='edit-<%= x.id%>'></div>
        <% end %>
      </tbody>
    </table>
  </div>
  
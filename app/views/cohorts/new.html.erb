<%= form_for @cohort do |f| %>  
    <ol><%= f.label :name %></ol>
    <ol><%= f.text_field :name, value: @cohort.name %></ol>  
    <ol><%= f.label :start_date %></ol>  
    <ol><%= f.date_field :start_date, value: @cohort.start_date %></ol>
    <ol><%= f.label :end_date %></ol>  
    <ol><%= f.date_field :end_date, value: @cohort.end_date %></ol>
    <ol><%= f.label :instructor_id %></ol>  
    <ol><%= f.number_field :instructor_id, value: @cohort.instructor_id %></ol>
    <ol><%= f.label :course_id %></ol>  
    <ol><%= f.number_field :cohort_id, value: @cohort.course_id %></ol>
    <p>
    <ol><%= f.submit %></ol>
<% end %>


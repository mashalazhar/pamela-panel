<%= form_for @instructor do |f| %>  
    <ol><%= f.label :first_name %></ol>
    <ol><%= f.text_field :first_name, value: @instructor.first_name %></ol>
    <ol><%= f.label :last_name %></ol> 
    <ol><%= f.text_field :last_name, value: @instructor.last_name %></ol>
    <ol><%= f.label :age %></ol>  
    <ol><%= f.date_field :age, value: @instructor.age %></ol>
    <ol><%= f.label :salary %></ol>  
    <ol><%= f.number_field :salary, value: @instructor.salary %></ol>
    <ol><%= f.label :education %></ol>  
    <ol><%= f.number_field :education, value: @instructor.education %></ol>
    <ol><%= f.label :cohort_id %></ol>  
    <ol><%= f.number_field :cohort_id, value: @instructor.cohort_id %></ol>
    <ol><%= f.submit %></ol>
<% end %>

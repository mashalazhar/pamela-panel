<div class="container">
  <h2><%= @instructor.first_name %> <%= @instructor.last_name %></h2>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Salary</th>
        <th>Education</th>
        <th>Cohort Name</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @instructor.first_name %></td>
        <td><%= @instructor.last_name %></td>
        <td><%= @instructor.age %></td>
        <td><%= @instructor.salary %></td>
        <td><%= @instructor.education %></td>
        <td><%= @instructor.cohort.name %></td>
        </tr>
    </tbody>
  </table>
</div>
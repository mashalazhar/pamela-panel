<%= form_for @instructor do |f| %>  
    <%= f.label :first_name %>  
    <%= f.text_field :first_name, value: @instructor.first_name %>  
    <%= f.label :last_name %>  
    <%= f.text_field :last_name, value: @instructor.last_name %>
    <%= f.label :age %>  
    <%= f.number_field :age, value: @instructor.age %>
    <%= f.label :salary %>  
    <%= f.number_field :salary, value: @instructor.salary %>
    <%= f.label :education %>  
    <%= f.text_field :education, value: @instructor.education %>
    <%= f.label :cohort_id %>  
    <%= f.number_field :cohort_id, value: @instructor.cohort_id %>
    <%= f.submit %>
<% end %>
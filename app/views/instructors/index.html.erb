<div class="container">
    <h2>Instructors</h2>          
    <table class="table table-striped">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Age</th>
          <th>Salary</th>
          <th>Education</th>
          <th>Cohort ID</th>
        </tr>
      </thead>
      <tbody>
      
        <tr>
        <% @instructors.each do |x| %>
          <td><%= x.first_name %></td>
          <td><%= x.last_name %></td>
          <td><%= x.age %></td>
          <td><%= x.salary %></td>
          <td><%= x.education %></td>
          <td><%= Cohort.find(x.cohort_id).name %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>

